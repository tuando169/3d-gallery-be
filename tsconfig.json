{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2016",
    "rootDir": "src",
    "outDir": "dist",
    "noImplicitAny": true,
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
